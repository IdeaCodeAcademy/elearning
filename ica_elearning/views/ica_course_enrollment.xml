<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ica_course_enrollment_view_tree" model="ir.ui.view">
        <field name="name">ica_course_enrollment_view_tree</field>
        <field name="model">ica.course.enrollment</field>
        <field name="arch" type="xml">
            <tree string="ica_course_enrollment_tree">
                <field name="partner_id"/>
                <field name="course_id"/>
            </tree>
        </field>
    </record>

    <record id="ica_course_enrollment_action" model="ir.actions.act_window">
        <field name="name">Enrollment</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">ica.course.enrollment</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>